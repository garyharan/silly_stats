<h1>Statistics</h1>

<table>
  <thead>
    <tr>
      <td>Table</td>
      <td>Count</td>
    </tr>
  </thead>
  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td>
          <%= model.name %>
        </td>
        <td>
          <%= model.count %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
